{
    "manifest_version": 3,
    "name": "Neat Code Grabber",
    "version": "1.0",
    "description": "",
    "action": {
        "default_popup": "index.html"
    },
    "permissions": [
        "scripting",
        "tabs",
        "activeTab"
    ],
    "host_permissions": [
        "https://*/*",
        "http://*/*"
    ],
    "icons": {
        "16": "images/16x16.png",
        "32": "images/32x32.png",
        "64": "images/64x64.png",
        "128": "images/128x128.png"
    }
  }